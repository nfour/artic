<!DOCTYPE html>
<html>
	<head>
		<title><<= @text.title >></title>
		<< if @text.description : >>
			<meta content="<<= @text.description >>" name="description">
		<< end >>
		<link media="screen" type="text/css" href="<<= @settings.url >>/static/style.css" rel="stylesheet" />
	</head>
	<body>
		<div class="wrapper">
			<< include 'header' >>
			<< content >>
			<< include 'footer' >>
		</div>
	</body>
</html>